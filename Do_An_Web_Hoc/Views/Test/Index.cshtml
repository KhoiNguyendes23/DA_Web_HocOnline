@model IEnumerable<Do_An_Web_Hoc.Models.UserAccount>

<form asp-action="Index" asp-controller="Test" method="post">
    <label for="roleId">Nhập Role ID:</label>
    <input type="number" id="roleId" name="roleId" required />
    <button type="submit">Tìm kiếm</button>
</form>

@if (Model != null)
{
    <table class="table">
        <thead>
            <tr>
                <th>UserID</th>
                <th>UserName</th>
                <th>Email</th>
                <th>FullName</th>
                <th>Status</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                    <td>@user.UserID</td>
                    <td>@user.UserName</td>
                    <td>@user.Email</td>
                    <td>@user.FullName</td>
                    <td>@user.Status</td>
                </tr>
            }
        </tbody>
    </table>
}
